<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duoshilin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".okhttp3.Okhttp3Activity"></activity>
        <activity android:name=".okhttp2.Okhttp2Activity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".rxjavaDemo.RxjavaActivity"
            android:label="@string/title_activity_rxjava"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".retrofitdemo.RetrofitActivity"
            android:label="@string/title_activity_retrofit"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>